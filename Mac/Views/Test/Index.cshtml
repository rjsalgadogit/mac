
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div id="table-container"></div>

<script>
    $(document).ready(function () {

        loadGrid('table-container'
            , '/Home/GetMacAddresses'
            , '/Home/GetMacAddress'
            , '/Home/DeleteMacAddress'
            , '/Home/SaveMacAddress'
            , 5,
            {
                TableId: 'mac_table',
                Columns: [
                    { DataField: 'Id', DataType: 'int', IsKey: true },
                    { DataField: 'Address', DataType: 'string', IsKey: false },
                    { DataField: 'Description', DataType: 'string', IsKey: false }
                ]
            });
    });
</script>